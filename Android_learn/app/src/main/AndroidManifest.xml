<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_PROFILE" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.videoEditor.CameraActivity"
            tools:ignore="InnerclassSeparator" />
        <activity android:name="com.videoEditor.VideoEditorActivity"
            tools:ignore="InnerclassSeparator" />
        <activity android:name="com.UILearn.PaintActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name="com.UILearn.RecyclerViewActivity"
            android:label="@string/title_activity_recycler_view"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar"
            tools:ignore="InnerclassSeparator" />
        <activity android:name="com.ThreadLearn.ThreadLearnActivity"
            tools:ignore="InnerclassSeparator" />
        <activity android:name="com.UILearn.DrawableMainActivity"
            tools:ignore="InnerclassSeparator" />
        <activity android:name="com.UILearn.ContactsOperationActivity"
            tools:ignore="InnerclassSeparator" />

        <activity android:name="com.UILearn.LayoutInflaterActivity"
            tools:ignore="InnerclassSeparator" />

        <activity android:name="com.common.ServiceActivity" />

        <activity android:name="com.UILearn.FragmentLearnActivity"
            tools:ignore="InnerclassSeparator" />

        <activity android:name="com.yyg.RJavaLearn.RXJavaLearnAvtivity"
            tools:ignore="InnerclassSeparator" />
        <activity android:name="com.yyg.kotlinlearn.FunctionAndLambdaActivity" />
        <activity android:name="com.UILearn.WebViewActivity"
            tools:ignore="InnerclassSeparator" />
        <activity android:name="com.yyg.kotlinlearn.EnumClassActivity" />
        <activity android:name="com.yyg.kotlinlearn.GenericityActivity" />
        <activity android:name="com.yyg.kotlinlearn.DataAndSealedActivity" />
        <activity android:name="com.yyg.kotlinlearn.ExtensionAvtivity" />
        <activity android:name="com.yyg.kotlinlearn.InterfaceActivity" />
        <activity android:name="com.yyg.kotlinlearn.InheritActivity" />
        <activity android:name="com.yyg.kotlinlearn.ClassAndObjectActivity" />
        <activity android:name="com.yyg.kotlinlearn.CycleControlActivity" />
        <activity android:name="com.yyg.kotlinlearn.ConditionControlActivity" />
        <activity android:name="com.yyg.kotlinlearn.BasicDataTypeActivity" />
        <activity android:name="com.yyg.kotlinlearn.BasicSyntaxActivity" />
        <activity android:name="com.yyg.kotlinlearn.KotlinLearn" />
        <activity android:name="com.UILearn.DrawDrawableActivity"
            tools:ignore="InnerclassSeparator" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.UILearn.ListViewLearn"
            tools:ignore="InnerclassSeparator" />
        <receiver
            android:name="com.common.NetWorkReceiver"
            android:enabled="true"
            android:exported="true"
            tools:ignore="ExportedReceiver" />
        <service
            android:name="com.common.MyLearnService"
            android:enabled="true"/>
        <service
            android:name="com.common.MyLearnRemoteService"
            android:enabled="true"
            android:exported="true"
            android:process=":remote">
            <intent-filter>
                <action android:name="com.yyg.service.LEARN_REMOTE_SERVICE" />
            </intent-filter>
        </service>

    </application>

</manifest>